if state.scene == "splash"
	@splash_scene (change_scene->change_scene)
else if state.scene == "game"
	@game_scene [level: state.level, high_score: state.high_score, score: state.score]
else if state.scene == "end"
	@end_scene (change_scene->change_scene) [score: state.score, high_score: state.high_score]
